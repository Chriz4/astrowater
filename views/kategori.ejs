<div class="kategori">

  <h2 class="text-center" id="kategori"><a href="#kategori" style="text-decoration: none; color: black;"> Kategori barang</a></h2>
  <div class="row mt-3">

    <% kategori_list.forEach((data, index) => { %>
    <div class="col-lg-3 col-md-6 col-sm-6 text-center my-4">
      <div class="card" style=" background-color: #6EEAD8;"> <!-- color alt #beea6e-->
        <img class="card-img-top" src=<%= data.img_source %> alt=<%= data.nama %>>
        <div class="card-body">
          <p class="card-text" style="font-weight: bold;"> <%= data.nama %> </p>
        </div>
      </div>
    </div>
    <% }) %>

  </div>
</div>